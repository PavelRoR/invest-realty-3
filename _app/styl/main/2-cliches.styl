#cliches
	padding 60px 0
	@media screen and (max-width 991px)
		padding 60px 0 30px
	@media screen and (max-width 560px)
		padding 40px 0 20px
.general-title-cliches
	padding 0 210px
	background-image: url(../img/icons/title-img-1.png), url(../img/icons/title-img-2.png)
	background-repeat: no-repeat, no-repeat
	background-position: left center, right center
	@media screen and (max-width 991px)
		padding 0 100px
		background-size: 90px auto
	@media screen and (max-width 767px)
		padding 0
		width 120%
		left -10%
	@media screen and (max-width 386px)
		width 100%
		left 0
		background-image: none
	
.cliche
	&-wrapper
		border-radius: 8px
		padding 8px
		background-color: #232323
		background-image: linear-gradient( -48deg, #232323 0%, #0c0c0c 100%);
		box-shadow: 0px 1px 53.1px 5.9px rgba(0, 0, 0, 0.68);
		width 100%
		max-width: 286px;
		margin 0 auto
		@media screen and (max-width 991px)
			margin-bottom: 30px
		@media screen and (max-width 560px)
			margin-bottom: 20px
	border-radius: 8px
	background-color: transparent
	border 2px dashed rgba(#d2c2a0, .3)
	padding 23px 5px
	position relative
	text-align center
	min-height 381px
	@media screen and (max-width 1199px)
		min-height 429px
	@media screen and (max-width 991px)
		min-height 1px
	&-title
		position absolute
		font-family: Ab
		font-size: 30px
		text-transform uppercase
		color #d2c2a0
		opacity .3
		top 0
		left 10px
	&-img
		display block
		margin 0 auto 20px
	&-text
		font-size: 16px
		span
			display block
			margin-bottom: 35px
			color #d2c2a0
			@media screen and (max-width 560px)
				margin-bottom: 20px